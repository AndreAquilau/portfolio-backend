{
   "name": "portfolio-backend",
   "version": "1.0.0",
   "main": "index.js",
   "author": "AndreAquilau <andre.aquilau@outlook.com>",
   "license": "MIT",
   "devDependencies": {
      "@babel/cli": "^7.12.10",
      "@babel/core": "^7.12.10",
      "@babel/node": "^7.12.10",
      "@babel/plugin-proposal-class-properties": "^7.12.1",
      "@babel/plugin-proposal-decorators": "^7.12.1",
      "@babel/preset-env": "^7.12.10",
      "@babel/preset-typescript": "^7.12.7",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^4.9.1",
      "@typescript-eslint/parser": "^4.9.1",
      "babel-plugin-module-resolver": "^4.0.0",
      "eslint": "^7.15.0",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-prettier": "^3.2.0",
      "prettier": "^2.2.1",
      "ts-node-dev": "^1.1.1",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.3.3333",
      "ts-node": "3.3.0"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "dev": "ts-node-dev --require tsconfig-paths/register --respawn --transpile-onle --ignore-watch node_modules --no-notity ./src/services/server.ts",
      "install:eslint": "yarn add -D @typescript-eslint/eslint-plugin eslint-config-airbnb-base eslint-plugin-import @typescript-eslint/parser",
      "build": "npx babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
      "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./ormconfig.js "
   },
   "dependencies": {
      "dotenv": "^8.2.0",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.29",
      "uuidv4": "^6.2.6",
      "mysql": "^2.14.1"
   }
}
